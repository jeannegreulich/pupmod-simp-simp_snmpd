# This file is managed by Puppet.  Changes to it will be over written
# Changes to access should be done in hiera
# or additions can be added in  /etc/snmpd/snmpd.d
#  views
[snmp] localCert <%= @_localcert %>
[snmp] tlsAlgorithms <%= @_ciphers %>
trustCert = <%= @_trustcert %>

certSecName 10 <%= @_tsmreadfp %> --sn <%= @_tsmreaduser %>
certSecName 10 <%= @_tsmwritefp %> --sn <%= @_tsmwriteuser %>

rouser -s tsm <%= @_tsmreaduser %>
rwuser -s tsm <%= @_tsmwriteuser %>
